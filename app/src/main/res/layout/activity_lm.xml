<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".LM">

    <androidx.camera.view.PreviewView
        android:id="@+id/viewFinder"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_weight="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </androidx.camera.view.PreviewView>

    <ImageButton
        android:id="@+id/lm0"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="50dp"
        android:background="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_lm0" />

    <ImageButton
        android:id="@+id/lm1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lm0"
        app:srcCompat="@mipmap/ic_lm1" />

    <ImageButton
        android:id="@+id/lm2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lm1"
        app:srcCompat="@mipmap/ic_lm2" />

    <ImageButton
        android:id="@+id/lm3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lm2"
        app:srcCompat="@mipmap/ic_lm3" />

    <ImageButton
        android:id="@+id/lm4"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lm3"
        app:srcCompat="@mipmap/ic_lm4" />

    <ImageButton
        android:id="@+id/lm5"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lm4"
        app:srcCompat="@mipmap/ic_lm5" />

    <ImageButton
        android:id="@+id/lm6"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="50dp"
        android:background="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_lm6" />

    <ImageButton
        android:id="@+id/lm7"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lm6"
        app:srcCompat="@mipmap/ic_lm7" />

    <ImageButton
        android:id="@+id/lm8"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lm7"
        app:srcCompat="@mipmap/ic_lm8" />

    <ImageButton
        android:id="@+id/lm9"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lm8"
        app:srcCompat="@mipmap/ic_lm9" />

    <ImageButton
        android:id="@+id/lm10"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lm9"
        app:srcCompat="@mipmap/ic_lm10" />

    <ImageButton
        android:id="@+id/lm11"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:background="@null"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lm10"
        app:srcCompat="@mipmap/ic_lm11" />

    <ImageButton
        android:id="@+id/bGrabarLM"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@null"
        android:contentDescription="Grabar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.916"
        app:srcCompat="@mipmap/ic_grabar256" />

    <Button
        android:id="@+id/bSiguienteLM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        android:alpha="0"
        android:text="Siguiente"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/bVolverLM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="20dp"
        android:text="Volver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tempLM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:alpha="0"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="#FF0000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>